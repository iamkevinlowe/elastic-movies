{
	"name": "elastic-movies-app",
	"version": "1.0.0",
	"description": "Movies directory using Elasticsearch",
	"main": "index.js",
	"scripts": {
		"start": "npm run build & npm run serve",
		"start:dev": "npm run webpack:dev & npm run serve:dev",
		"serve": "node index.js",
		"serve:dev": "nodemon --ignore worker --ignore src index.js",
		"worker": "node worker/index.js",
		"worker:dev": "nodemon --ignore routers --ignore src --trace-warnings worker/index.js",
		"build": "webpack --config webpack.config.js --env NODE_ENV=production --mode production",
		"build:dev": "webpack --config webpack.config.js --env NODE_ENV=development --mode development",
		"webpack:dev": "webpack serve --env NODE_ENV=development --history-api-fallback --devtool source-map",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@babel/core": "^7.12.10",
		"@babel/preset-env": "^7.12.11",
		"@babel/preset-react": "^7.12.10",
		"@elastic/elasticsearch": "^7.9.1",
		"@popperjs/core": "^2.6.0",
		"babel-loader": "^8.2.2",
		"body-parser": "^1.19.0",
		"bootstrap": "^4.5.3",
		"bull": "^3.20.0",
		"bull-board": "^1.2.0",
		"core-js": "^3.8.1",
		"css-loader": "^5.0.1",
		"express": "^4.17.1",
		"html-webpack-plugin": "^4.5.0",
		"jquery": "^3.5.1",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-router-dom": "^5.2.0",
		"regenerator-runtime": "^0.13.7",
		"sass": "^1.32.0",
		"sass-loader": "^10.1.0",
		"style-loader": "^2.0.0",
		"webpack": "^5.11.1",
		"webpack-cli": "^4.3.1"
	},
	"devDependencies": {
		"nodemon": "^2.0.6",
		"webpack-dev-server": "^3.11.1"
	}
}
