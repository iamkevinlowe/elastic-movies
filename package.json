{
	"name": "elastic-movies",
	"version": "1.0.0",
	"description": "Movies directory using Elasticsearch",
	"repository": {
		"type": "git",
		"url": "https://github.com/iamkevinlowe/elastic-movies.git"
	},
	"main": "server/index.js",
	"scripts": {
		"build:dev": "docker-compose -f docker/docker-compose.dev.yml build",
		"up:dev": "docker-compose -f docker/docker-compose.dev.yml up",
		"down:dev": "docker-compose -f docker/docker-compose.dev.yml down",
		"uninstall:dev": "docker-compose -f docker/docker-compose.dev.yml down --rmi all",
		"job:index:start": "node server/jobs/indexPopularMovies.js",
		"job:index:debug": "DEBUG=true node server/jobs/indexPopularMovies.js",
		"test": "echo \"Error: no test specified\" && exit 1",
		"api:dev": "nodemon server/index.js",
		"api:prod": "node server/index.js",
		"app:dev": "webpack serve --env NODE_ENV=development --history-api-fallback --devtool source-map",
		"app:build:dev": "webpack --config ./webpack.config.js --mode development",
		"app:build:prod": "webpack --mode production"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@elastic/elasticsearch": "^7.9.1",
		"body-parser": "^1.19.0",
		"express": "^4.17.1"
	},
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/preset-env": "^7.12.10",
		"@babel/preset-react": "^7.12.10",
		"@popperjs/core": "^2.6.0",
		"babel-loader": "^8.2.2",
		"bootstrap": "^5.0.0-beta1",
		"core-js": "^3.8.1",
		"css-loader": "^5.0.1",
		"html-webpack-plugin": "^4.5.0",
		"nodemon": "^2.0.6",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-router-dom": "^5.2.0",
		"regenerator-runtime": "^0.13.7",
		"sass": "^1.30.0",
		"sass-loader": "^10.1.0",
		"style-loader": "^2.0.0",
		"webpack": "^5.10.1",
		"webpack-cli": "^4.2.0",
		"webpack-dev-server": "^3.11.0"
	}
}
